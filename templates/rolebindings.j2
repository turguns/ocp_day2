apiVersion: rbac.authorization.k8s.io/v1
kind: RoleBinding
metadata:
  name: {{ item.rolebindings }}-{{ item.adgroups }}
  namespace: {{ item.project_name  }}
roleRef:
  apiGroup: rbac.authorization.k8s.io
  kind: ClusterRole
  name: {{ item.rolebindings }}
subjects:
- apiGroup: rbac.authorization.k8s.io
  kind: Group
  name: {{ item.adgroups }}
